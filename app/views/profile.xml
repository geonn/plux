<Alloy>
	<Window navTintColor="#CE1D1C" title="My Profile" id="myProfile" class="container" layout="vertical">
		<View layout="vertical" height="100%">
			<View layout="horizontal" height="50" width="Ti.UI.FILL" backgroundColor="#DEDEDE" platform="android" > 
				<View left="0" width="10%">
		         	<ImageView left='10' id="btnBack" width="25" height="25" image="/images/btn-back.png" />
		   		</View>
		     	<View id="pageTitle" width="90%">
		     		<Label height="Ti.UI.SIZE" class='normal_font' textAlign="center">My Profile</Label>
		     	</View>
		 	</View>
        	 <Label id="description" class="label" text="Please choose profile of the below services" />
        	 <ScrollView id="scrollboard" width="Titanium.UI.FILL" height="Ti.UI.FILL" zIndex="3">
				<View layout="horizontal" width="293" top="20">
					<ImageView top="15" borderRadius="10" width="120" left="15" height="Ti.UI.SIZE" onClick="navProfile" mod="plux" backgroundColor="#ff0000" bottom="30dp" top="30dp" image="/images/logo_plux.png"></ImageView>
					<ImageView top="15" borderRadius="10" width="120" left="15" height="Ti.UI.SIZE" onClick="navProfile" mod="asp" backgroundColor="#ff0000" bottom="30dp" top="30dp" image="/images/asp_logo.png"></ImageView>
				</View>
			</ScrollView> 
		</View>
	</Window>
</Alloy>